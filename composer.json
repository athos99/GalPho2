{
    "name": "athos99/galpho2",
    "description": "Gallery photo",
    "keywords": ["image", "gallery", "photo", "application"],
    "homepage": "http://www.athos99.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },

    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "athos99/assetparser",
                "description": "Asset parser Less scss and sass for the Yii framework",
                "keywords": ["yii2", "less", "sass", "scss"],
                "type": "yii2-extension",
                "license": "BSD-3-Clause",
                "installation-source": "dist",

                "support": {
                    "source": "https://github.com/athos99/assetparser"
                },
                "authors": [
                    {
                        "name": "Michel Bobillier aka Athos99",
                        "email": "athos99@bluewin.ch"
                    }
                ],
                "version": "master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/athos99/assetparser.git",
                    "reference": "master"

                },

                "dist": {
                    "type": "zip",
                    "url": "https://github.com/athos99/assetparser/archive/master.zip",
                    "reference": "master"

                },
                "require": {
                    "leafo/lessphp": "0.4.*@dev",
                    "leafo/scssphp": "dev-master",
                    "richthegeek/phpsass": "dev-master"
                },
                "autoload": {
                    "psr-4": {
                        "athos99\\assetparser\\": ""
                    }
                }

            }
        }


        ,




        {
            "type": "package",
            "package": {
                "name": "athos99/plupload",
                "description": "Asset parser Less scss and sass for the Yii framework",
                "keywords": ["yii2", "less", "sass", "scss"],
                "type": "yii2-extension",
                "license": "BSD-3-Clause",
                "installation-source": "dist",

                "support": {
                    "source": "https://github.com/athos99/plupload"
                },
                "authors": [
                    {
                        "name": "Michel Bobillier aka Athos99",
                        "email": "athos99@bluewin.ch"
                    }
                ],
                "version": "master",
                "source": {
                    "type": "git",
                    "url": "https://github.com/athos99/plupload.git",
                    "reference": "master"

                },

                "dist": {
                    "type": "zip",
                    "url": "https://github.com/athos99/plupload/archive/master.zip",
                    "reference": "master"

                },
                "require": {
                    "leafo/lessphp": "0.4.*@dev",
                    "leafo/scssphp": "dev-master",
                    "richthegeek/phpsass": "dev-master"
                },
                "autoload": {
                    "psr-4": {
                        "athos99\\plupload\\": ""
                    }
                }

            }
        }



    ],
    "minimum-stability": "dev",


    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-authclient": "*",
        "yiisoft/yii2-jui": "*",
        "vitalets/x-editable": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "leafo/lessphp": "0.4.*@dev",
        "leafo/scssphp": "dev-master",
        "richthegeek/phpsass": "dev-master",
        "athos99/assetparser": "*",
        "athos99/plupload": "*"
    },
    "suggest": {


    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission"
        ]
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "writable": [
            "runtime",
            "web/assets"
        ],
        "executable": [
            "yii"
        ],
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }

    }
}

